{
    "org": "liatrio-enterprise",
    "name": "Repo Level Ruleset",
    "target": "branch",
    "enforcement": "active",
    "bypass_actors": [
      {
          "actor_id": 1,
          "actor_type": "OrganizationAdmin",
          "bypass_mode": "always"
      },
      {
        "actor_id": 166418,
        "actor_type": "Integration",
        "bypass_mode": "always"
      }
    ],
    "conditions": {
        "ref_name": {
            "include": ["~ALL"],
            "exclude": []
        }
    },
    "rules": [
        {
            "type": "required_status_checks",
            "parameters": {
                "strict_required_status_checks_policy": false,
                "required_status_checks": [
                    {
                        "context": "CodeQL"
                    }
                ]
            }
        },
        {
            "type": "code_scanning",
            "parameters": {
                "code_scanning_tools": [
                    {
                        "tool": "CodeQL",
                        "security_alerts_threshold": "high_or_higher",
                        "alerts_threshold": "errors"
                    }
                ]
            }
        }
    ]
  }